package main

import (
"fmt"
"reflect"
)

type User struct {
ID int
Name string
Age int
}


func (u User) Hello(name string){
fmt.println("Hello",name,"my name is ",u.name)
}

fuc main(){
u := User{1,"ok",12}
v := reflect.ValueOf(u)
mv := v.MethodByName("Hello")

args := []reflect.Value{reflect.ValueOf("joe")}

mv.Call(args)




}
